{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "background": "#121414",
    "error": "#ffb4ab",
    "error_container": "#93000a",
    "inverse_on_surface": "#2f3130",
    "inverse_primary": "#506162",
    "inverse_surface": "#e3e2e2",
    "on_background": "#e3e2e2",
    "on_error": "#690005",
    "on_error_container": "#ffdad6",
    "on_primary": "#233334",
    "on_primary_container": "#d4e6e7",
    "on_secondary": "#293233",
    "on_secondary_container": "#dae4e5",
    "on_surface": "#e3e2e2",
    "on_surface_variant": "#c7c6c6",
    "on_tertiary": "#1b3436",
    "on_tertiary_container": "#cce8e9",
    "outline": "#919190",
    "outline_variant": "#464747",
    "primary": "#b8cacb",
    "primary_container": "#394a4b",
    "scrim": "#000000",
    "secondary": "#bec8c9",
    "secondary_container": "#3f4949",
    "shadow": "#000000",
    "surface": "#121414",
    "surface_bright": "#383939",
    "surface_container": "#1f2020",
    "surface_container_high": "#292a2a",
    "surface_container_highest": "#343535",
    "surface_container_low": "#1b1c1c",
    "surface_container_lowest": "#0d0e0e",
    "surface_dim": "#121414",
    "surface_tint": "#b8cacb",
    "surface_variant": "#464747",
    "tertiary": "#b1cccd",
    "tertiary_container": "#324b4c"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:primary",
          "foreground": "p:on_primary",
          "options": {
            "style": "austin",
            "threshold": 321
          },
          "style": "plain",
          "template": " {{ .FormattedMs }} ",
          "type": "executiontime"
        },
        {
          "background": "p:error_container",
          "foreground": "p:on_error_container",
          "style": "plain",
          "template": "{{ if gt .Code 0 }} {{ .Code }} {{ end }}",
          "type": "status"
        },
        {
          "background": "p:tertiary",
          "foreground": "p:on_tertiary",
          "style": "plain",
          "template": "\uf0e7",
          "type": "root"
        },
        {
          "background": "p:surface",
          "foreground": "p:on_surface",
          "style": "plain",
          "template": " {{ if .WSL }}\uebcc {{ end }}{{.Icon}}",
          "type": "os"
        },
        {
          "background": "p:surface",
          "foreground": "p:on_surface",
          "style": "plain",
          "template": " {{ if .SSHSession }}\ueba9 {{ end }}{{ .UserName }}@{{ .HostName }} ",
          "type": "session"
        },
        {
          "background": "p:surface_variant",
          "foreground": "p:on_surface_variant",
          "options": {
            "folder_icon": "\u2026",
            "style": "mixed"
          },
          "style": "plain",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "p:secondary_container",
          "foreground": "p:on_secondary_container",
          "options": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "style": "plain",
          "template": " {{ .HEAD }} ",
          "type": "git"
        },
        {
          "background": "p:tertiary_container",
          "foreground": "p:on_tertiary_container",
          "options": {
            "fetch_version": false
          },
          "style": "plain",
          "template": " \ue77f ",
          "type": "dotnet"
        },
        {
          "background": "p:tertiary_container",
          "foreground": "p:on_tertiary_container",
          "options": {
            "fetch_version": false
          },
          "style": "plain",
          "template": " \ue626 ",
          "type": "go"
        },
        {
          "background": "p:tertiary_container",
          "foreground": "p:on_tertiary_container",
          "options": {
            "fetch_version": false
          },
          "style": "plain",
          "template": " \ue235 ",
          "type": "python"
        },
        {
          "background": "p:tertiary_container",
          "foreground": "p:on_tertiary_container",
          "options": {
            "fetch_version": false
          },
          "style": "plain",
          "template": " \ue7a8 ",
          "type": "rust"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:primary",
          "style": "plain",
          "template": "~> ",
          "type": "shell"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 4
}